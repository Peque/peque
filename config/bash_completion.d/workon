_workon() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    #opts=$(conda info --envs | grep envs | awk '{print $1}')
    opts=$(ls /cache/miniconda/envs/)

    COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
}
complete -F _workon workon
